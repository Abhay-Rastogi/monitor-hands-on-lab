{
    "cards": [
      {
          "content": {
              "series": [
                  {
                      "dataSourceId": "travel_time",
                      "label": "Travel Time"
                  }
              ],
              "unit": "sec"
          },
          "dataSource": {
              "attributes": [
                  {
                      "attribute": "travel_time",
                      "id": "travel_time"
                  }
              ]
          },
          "id": "travel_time_line_card",
          "size": "LARGE",
          "title": "Travel Time",
          "type": "TIMESERIES"
      },
        {
            "content": {
                "series": [
                    {
                        "dataSourceId": "speed_mean_ID",
                        "label": "Average Speed"
                    },
                    {
                        "dataSourceId": "speed_max_ID",
                        "label": "Maximum Speed"
                    },
                    {
                        "dataSourceId": "speed_min_ID",
                        "label": "Minimum Speed"
                    }
                ],
                "unit": "m/sec"
            },
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "mean",
                        "attribute": "speed",
                        "id": "speed_mean_ID"
                    },
                    {
                        "aggregator": "max",
                        "attribute": "speed",
                        "id": "speed_max_ID"
                    },
                    {
                        "aggregator": "min",
                        "attribute": "speed",
                        "id": "speed_min_ID"
                    }
                ]
            },
            "id": "speedLayoutID",
            "size": "LARGE",
            "title": "Minimum Speed vs Maximum Speed vs Average Speed",
            "type": "TIMESERIES"
        },
        {
            "content": {
                "attributes": [
                    {
                        "dataSourceId": "torque_min_Id",
                        "precision": 3,
                        "label": "Minimum torque",
                        "thresholds": [
                            {
                                "color": "#5aa700",
                                "comparison": "<",
                                "icon": "icon--checkmark--outline",
                                "value": 10
                            },
                            {
                                "color": "#F00",
                                "comparison": ">",
                                "icon": "icon--warning",
                                "value": 10
                            }
                        ]
                    },
                    {
                        "dataSourceId": "torque_max_Id",
                        "precision": 3,
                        "label": "Maximum torque",
                        "thresholds": [
                            {
                                "color": "#5aa700",
                                "comparison": "<",
                                "icon": "icon--checkmark--outline",
                                "value": 14
                            },
                            {
                                "color": "#F00",
                                "comparison": ">",
                                "icon": "icon--warning",
                                "value": 14
                            }
                        ]
                    }
                ]
            },
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "min",
                        "attribute": "torque",
                        "id": "torque_min_Id"
                    },
                    {
                        "aggregator": "max",
                        "attribute": "torque",
                        "id": "torque_max_Id"
                    }
                ]
            },
            "id": "torqueLayoutId",
            "size": "SMALL",
            "title": "Torque (N.m)",
            "type": "VALUE"
        },
        {
            "content": {
                "attributes": [
                    {
                        "dataSourceId": "speed_last_Id",
                        "precision": 3,
                        "label": "Current Speed",
                        "thresholds": [
                            {
                                "color": "#5aa700",
                                "comparison": "<",
                                "icon": "icon--checkmark--outline",
                                "value": 5
                            },
                            {
                                "color": "#F00",
                                "comparison": ">",
                                "icon": "icon--warning",
                                "value": 5
                            }
                        ]
                    }
                ]
            },
            "dataSource": {
                "attributes": [
                    {
                        "aggregator": "last",
                        "attribute": "speed",
                        "id": "speed_last_Id"
                    }
                ]
            },
            "id": "currentSpeedLayoutId",
            "size": "SMALL",
            "title": "Speed (m/sec)",
            "type": "VALUE"
        },
        {
            "content": {
                "attributes": [
                    {
                        "dataSourceId": "abnormal_stop_count_last_Id",
                        "precision": 3,
                        "thresholds": [
                            {
                                "color": "#5aa700",
